{
    "name": "zendframework/skeleton-application",
    "description": "Skeleton Application for Zend Framework zend-mvc applications",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "mvc",
        "zf"
    ],
    "homepage": "http://framework.zend.com/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5",
        "zendframework/zendframework": "2.*",
        "zf-commons/zfc-rbac": "~2.0",
        "widmogrod/zf2-assetic-module": "1.6.0",
        "doctrine/dbal": "2.*",
        "doctrine/doctrine-orm-module": "0.*",
        "doctrine/migrations": "^1.2",
        "doctrine/data-fixtures": "*",
        "zf-commons/zfc-base": "0.1.2",
        "newtonsnerds/goalio-rememberme": "1.*",
        "socalnick/scn-social-auth": "1.21.*"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/"
        }
    },
    "extra": [],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
        "test": "phpunit",
        "post-install-cmd": [
            "sh bin/submodules_init.sh",
            "sh bin/create_folder.sh",
            "sh bin/templatemap_gen.sh",
            "sh bin/submodules_init.sh"
        ],
        "post-update-cmd": [
            "sh bin/submodules_init.sh",
            "sh bin/create_folder.sh",
            "sh bin/templatemap_gen.sh",
            "sh bin/submodules_init.sh"
        ]
    }
}
